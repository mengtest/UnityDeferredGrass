// Vertex Shader
// -----------------------------------------------------------------------------

VertOutput VertMain(VertInput input)
{
    VertOutput output;
    
    output.position = input.position;
    output.normal   = input.normal;
    output.tangent  = input.tangent;
    output.uv       = input.uv;
    output.tex1     = input.tex1;
    output.tex2     = input.tex2;
    output.tex3     = input.tex3;

    return output;
}

